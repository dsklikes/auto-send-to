{
  "name": "@devvit/plugins",
  "version": "0.9.6",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://developers.reddit.com/"
  },
  "type": "module",
  "main": "./index.js",
  "scripts": {
    "build": "yarn build:gl && tsc",
    "build:gl": "gen-gl-proxy --typescript src/gl/GLProxy.ts",
    "clean": "rm -rf .turbo coverage dist src/gl/GLProxy.ts",
    "clobber": "yarn clean && rm -rf node_modules",
    "dev": "tsc -w",
    "lint": "redlint .",
    "lint:fix": "yarn lint --fix",
    "prepublishOnly": "publish-package-json",
    "test": "yarn test:unit && yarn test:types",
    "test:types": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:unit-with-coverage": "vitest run --coverage"
  },
  "types": "./index.d.ts",
  "dependencies": {
    "@devvit/protos": "0.9.6"
  },
  "devDependencies": {
    "@devvit/eslint-config": "0.9.6",
    "@devvit/repo-tools": "0.9.6",
    "@devvit/tsconfig": "0.9.6",
    "eslint": "8.9.0",
    "typescript": "4.9.3",
    "vitest": "0.31.0"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "source": "./src/index.ts",
  "gitHead": "656833a63ae45857f86b4753f46d9344430e3c8c"
}
